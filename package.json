{
    "name": "react-overflowed",
    "version": "0.0.2",
    "description": "Smooth draggable overflowed react container",
    "main": "build/index.js",
    "module": "build/index.es.js",
    "files": [
        "build"
    ],
    "scripts": {
        "build": "rollup -c",
