        "publish": "npm login && npm publish"
    },
    "author": "polmoneys",
    "license": "ISC",
    "devDependencies": {
        "@rollup/plugin-commonjs": "^17.1.0",
        "@rollup/plugin-node-resolve": "^11.2.0",
        "@types/react": "^17.0.3",
        "node-sass": "^5.0.0",
        "postcss": "^8.2.8",
        "prettier": "2.2.1",
        "rollup": "^2.41.5",
        "rollup-plugin-peer-deps-external": "^2.2.4",
        "rollup-plugin-postcss": "^4.0.0",
        "rollup-plugin-typescript2": "^0.30.0",
        "typescript": "^4.2.3"
    },
    "peerDependencies": {
        "react": ">=16.8.0",
        "react-dom": ">=16.8.0"
    },
    "dependencies": {
        "@reach/rect": "^0.13.2",
